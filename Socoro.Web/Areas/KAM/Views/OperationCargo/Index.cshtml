<script>
    $('[name = NoContainers]').on('change', function () {
        $('#containers').empty();
        var num = parseInt($(this).val());
        var dynamicForm = '';
        for (var n = 0; n < num; n++) {
            dynamicForm +=
                '<div class = "container" id="container'+n+'" style="border: 1px solid black;"> \
                     <div class="form-group"> \
                        <label class="col-form-label">Gross Weight</label>: \
                        <input asp-for="OperationContainerViewModel.GrossWeight"  name="ContainerGrossWeight' + n + '" type="number" class="form-control" placeholder="" /> \
                    </div> \
                    <div class="form-group"> \
                        <label class="col-form-label">Commercial Description</label>: \
                        <input asp-for="OperationContainerViewModel.CommercialDescription" name="ContainerCommercialDescription' + n + '" type="text" class="form-control" placeholder="" /> \
                    </div> \
                    <div class="form-group"> \
                        <label class="col-form-label">Container #' + n + ' Type</label>: \
                        <select name="ContainerTypeId" class="selectpicker form-control" data-ng-style="select-with-transition"></select> \
                        <span asp-validation-for="OperationContainerViewModel.ContainerTypeId" class="text-danger"></span> \
                    </div> \
                    <div class="form-group"> \
                        <label class="col-form-label">Is Container #' + n + ' Hazardous?</label>: \
                        <select name="Hazardous' + n + '" class="selectpicker form-control" data-ng-style="select-with-transition"> \
                            <option value="false" selected>No</option > \
                            <option value="true">Yes</option> \
                        </select> \
                    </div> \
                </div><br />';
        }
        if (num > 0) {
            $('#containers').css('display', 'block');
            var newDiv = document.createElement('div');
            newDiv.innerHTML = dynamicForm;
            document.getElementById('containers').appendChild(newDiv);
        } else {
            $('#containers').css('display', 'none');
        }
    });
</script>