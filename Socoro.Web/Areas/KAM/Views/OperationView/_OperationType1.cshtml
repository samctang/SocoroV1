                    <li>Loading Address</li>
                    <li>Container Information</li>
                    <li>Contact Information</li>
                    <li>Loading Date & Dock Hours</li>
                    <li>EIN</li>
                    <li>PickUp Reference</li>
                    <li>Bonded</li>
                    <li>Self Propelled</li>
                </ul>
                <div class="card card-custom" id="operationProcessType_Id_4_Task" style="display:none">
                    <div class="card-header" style="background-color: lavender">
                        <div class="card-title">
                            <h3 class="card-label">
                                Create Task for Operation #@Model.OperationViewModel.OperationNo
                            </h3>
                        </div>
                    </div>
                    <div class="card-body">
                        <form id="operationProcessType_Id_4_Task_Form">
                            <div class="form-group">
                                <label class="col-form-label notes" asp-for="OperationTaskViewModel.Note"></label>:
                                <br />
                                <textarea class="col-form-label" asp-for="OperationTaskViewModel.Note" id="operationProcessType_Id_4_Note"></textarea>
                                <br />
                                <label class="col-form-label" asp-for="OperationTaskViewModel.DueDate"></label>:
                                <br />
                                <input asp-for="OperationTaskViewModel.DueDate" name="DueDate" type="date" id="operationProcessType_Id_4_DueDate" class="form-control" placeholder="Due Date" pattern="\d{4}-\d{2}-\d{2}" />
                                <br />
                                <p id="operationProcessType_Id_4_Status_Display" style="display:none"><input asp-for="OperationTaskViewModel.Status" type="checkbox" id="operationProcessType_Id_4_Status" /> Mark as completed </p>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn font-weight-bold btn-light-danger cancelTask" name="operationProcessType_Id_4">Cancel</button>
                        <button type="submit" class="btn font-weight-bold btn-light-success submitTask" name="operationProcessType_Id_4" form="operationProcessType_Id_4_Task_Form">Add Task</button>
                        <button type="submit" class="btn font-weight-bold btn-light-primary editTask" name="operationProcessType_Id_4" form="operationProcessType_Id_4_Edit_Task_Form" style="display:none">Edit Task</button>
                    </div>
                </div>
                <div class="card card-custom" id="operationProcessType_Id_4_TaskList" style="display: none">
                    <div class="card-header" style="background-color: lightseagreen">
                        <div class="card-title">
                            <h3 class="card-label">
                                Tasks for this step
                            </h3>
                        </div>
                    </div>
                    <br />
                    <div class="card-body">
                        <div class="datatable datatable-bordered datatable-head-custom" id="kt_datatable_Task_4"></div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-info font-weight-bold mr-auto tasksBtn" name="operationProcessType_Id_4">Tasks</button>
                <input type="submit" class="btn btn-light-primary font-weight-bold" value="Send Request" />
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="operationProcessType_Id_2_Modal" tabindex="-1" role="dialog" aria-labelledby="staticBackdrop" aria-hidden="true">
