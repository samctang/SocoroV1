@model Socoro.Web.Areas.KAM.Models.OperationIntViewModel;

<script language="JavaScript" type="text/javascript" src="~/lib/jquery/jquery.min.js"></script>

<!--begin::Modals-->
<div class="modal fade" id="operationProcessType_Id_1_Modal" tabindex="-1" role="dialog" aria-labelledby="staticBackdrop" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Step 1: FCL - FOB</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <i aria-hidden="true" class="ki ki-close"></i>
                </button>
            </div>
            <div class="modal-body">
                <p>You have initiated a new process for:</p>
                <br />
                <p><strong>Agent</strong>: <span class="AgentCompany"></span></p>
                <p><strong>Shipper</strong>: <span class="ShipperCompany"></span></p>
                <p><strong>Consignee</strong>: <span class="ConsigneeCompany"></span></p>
                <br />
                <p>The shipper will be notified that you will be in charge of their operation in the next step.</p>
                <div class="card card-custom" id="operationProcessType_Id_1_Task" style="display:none">
                    <div class="card-header" style="background-color: lavender">
                        <div class="card-title">
                            <h3 class="card-label">
                                Create Task for Operation #@Model.OperationViewModel.OperationNo
                            </h3>
                        </div>
                    </div>
                    <div class="card-body">
                        <form id="operationProcessType_Id_1_Task_Form">
                            <div class="form-group">
                                <label class="col-form-label notes" asp-for="OperationTaskViewModel.Note"></label>:
                                <br />
                                <textarea class="col-form-label" asp-for="OperationTaskViewModel.Note" id="operationProcessType_Id_1_Note"></textarea>
                                <br />
                                <label class="col-form-label" asp-for="OperationTaskViewModel.DueDate"></label>:
                                <br />
                                <input asp-for="OperationTaskViewModel.DueDate" name="DueDate" type="date" id="operationProcessType_Id_1_DueDate" class="form-control" placeholder="Due Date" pattern="\d{4}-\d{2}-\d{2}" />
                                <br />
                                <p id="operationProcessType_Id_1_Status_Display" style="display:none"><input asp-for="OperationTaskViewModel.Status" type="checkbox" id="operationProcessType_Id_1_Status" /> Mark as completed </p>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn font-weight-bold btn-light-danger cancelTask" name="operationProcessType_Id_1">Cancel</button>
                        <button type="submit" class="btn font-weight-bold btn-light-success submitTask" name="operationProcessType_Id_1" form="operationProcessType_Id_1_Task_Form">Add Task</button>
                        <button type="submit" class="btn font-weight-bold btn-light-primary editTask" name="operationProcessType_Id_1" form="operationProcessType_Id_1_Edit_Task_Form" style="display:none">Edit Task</button>
                    </div>
                </div>
                <div class="card card-custom" id="operationProcessType_Id_1_TaskList" style="display: none">
                    <div class="card-header" style="background-color: lightseagreen">
                        <div class="card-title">
                            <h3 class="card-label">
                                Tasks for this step
                            </h3>
                        </div>
                    </div>
                    <br />
                    <div class="card-body">
                        <div class="datatable datatable-bordered datatable-head-custom" id="kt_datatable_Task_1"></div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-info font-weight-bold mr-auto tasksBtn" name="operationProcessType_Id_1">Tasks</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="operationProcessType_Id_4_Modal" tabindex="-1" role="dialog" aria-labelledby="staticBackdrop" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
